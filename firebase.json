{
  "hosting": [
    {
      "site": "brainiacwiz",
      "public": "./",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/**",
          "destination": "https://brainiacwiz.com"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/**",
      "destination": "https://brainiacwiz.com",
      "type": 301
    }
  ]
}