{
  "hosting": [
    {
      "site": "brainiacwiz",
      "public": "./",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ],
  "redirects": [
    {
      "source": "/**",
      "destination": "https://brainiacwiz.com",
      "type": 301
    },
    {
      "source": "https://brainiacwiz.web.app/**",
      "destination": "https://brainiacwiz.com",
      "type": 301
    }
  ]
}